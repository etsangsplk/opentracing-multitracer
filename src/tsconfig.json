{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": ".",
    "composite": true,
    "declarationMap": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": ["es2015", "es2016", "es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "outDir": "../dist",
    "target": "es5"
  }
}
