{
  "compilerOptions": {
    "alwaysStrict": true,
    "baseUrl": ".",
    "inlineSourceMap": true,
    "inlineSources": true,
    "lib": ["es2015", "es2016", "es2017"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "noUnusedLocals": false,
    "outDir": "./dist",
    "target": "es2018"
  },
  "references": [
    {
      "path": "./src"
    },
  ]
}
